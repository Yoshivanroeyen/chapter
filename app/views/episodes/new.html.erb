<div class="darken">
  <div class="container margin-top-90 book">
    <%= render 'form' %>
  </div>
  <div class="superbox-markdown">
    <ul>
      <li>
        <input type="checkbox" id="list-item-1">
        <label class="label-green" for="list-item-1" class="first">Emphasis</label>
        <ul>
          <li>
            <p>*Italic*</p>
            <p><em>Italic</em></p>
          </li>
          <li>
            <p>**Bold**</p>
            <p><strong>Bold</strong></p>
          </li>
          <li>
            <p>_Underscore_ </p>
            <p><u>Underscore</u></p>
          </li>
          <li>
            <p>*Also*_In_**Same**Word</p>
            <p><em>Also</em><u>In</u><strong>Same</strong>Word</li></p>
        </ul>
      </li>
      <li>
        <input type="checkbox" id="list-item-2">
        <label class="label-green" for="list-item-2">Titles</label>
        <ul>
          <li>
            <p>## Title</p>
            <h2>Title</h2>
          </li>
          <li>
            <p>### Title</p>
            <h3>Title</h3>
          </li>
        </ul>
      </li>
      <li>
        <input type="checkbox" id="list-item-3">
        <label class="label-green" for="list-item-3" class="last">Quoting</label>
        <ul>
          <li>
          <p> > Lorem ipsum dolor sit amet</p>
          <blockquote>
            <p> Lorem ipsum dolor sit amet</p>
            </blockquote>
          </li>
          <li>
          <p> > level one </p>
          <p> > > Level two </p>
          <p> > > > Level three </p>
          <blockquote><p>
          Level one
            </p>
            <blockquote><p>
              Level two
              </p>
              <blockquote><p>
                Level three
                </p>
              </blockquote>
            </blockquote>
          </blockquote>
          </li>
        </ul>
      </li>
    </ul>
  </div>
</div>

